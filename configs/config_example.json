{
  "debug": false,
  "foreground_color": "white",
  "background_color": "#26242f",
  "toast_color_warning": "#8B4513",
  "toast_color_success": "#2d5016",
  "locale": "en",
  "log_level": "info",
  "clip_model": "ViT-B/32",
  "compare_mode": "CLIP_EMBEDDING",
  "max_search_results": 200,
  "search_only_return_closest": false,
  "file_actions_history_max": 200,
  "file_actions_window_rows_max": 300,
  "store_checkpoints": false,
  "embedding_similarity_threshold": 0.9,
  "color_diff_threshold": 15,
  "escape_backslash_filepaths": true,
  "file_counter_limit": 40000,
  "fill_canvas": false,
  "screenshot_directory": null,
  "save_screenshot_to_same_dir": true,
  "image_browse_recursive": false,
  "sidebar_visible": true,
  "image_tagging_enabled": true,
  "print_settings": true,
  "show_toasts": true,
  "slideshow_interval_seconds": 7,
  "file_check_interval_seconds": 10,
  "file_check_skip_if_n_files_over": 5000,
  "default_main_window_size": "1400x950",
  "default_secondary_window_size": "600x700",
  "sort_by": "CREATION_TIME",
  "toasts_persist_seconds": 2,
  "title_notify_persist_seconds": 5,
  "delete_instantly": false,
  "clear_marks_with_errors_after_move": false,
  "move_marks_overwrite_existing_file": false,
  "marked_file_svg_move_type": "svg",
  "trash_folder": null,
  "always_open_new_windows": false,
  "sd_prompt_reader_loc": "{HOME}\\stable-diffusion-prompt-reader",
  "siglip_enable_large_model": false,
  "xvlm_loc": null,
  "xvlm_model_loc": null,
  "xvlm_model_size": "4m",
  "laion_enable_half_precision": false,
  "tag_suggestions_file": "tag_suggestions.json",
  "image_types": [
    ".jpg", ".jpeg", ".png", ".tif", ".tiff", ".webp", ".bmp", ".heic", ".avif", ".ico"
  ],
  "video_types": [
    ".mp4", ".mkv", ".webm", ".avi", ".mov", ".wmv", ".flv", ".mpeg", ".m4a"
  ],
  "enable_videos": true,
  "enable_gifs": true,
  "enable_pdfs": true,
  "enable_svgs": true,
  "enable_html": true,
  "directories_to_search_for_related_images": [    
  ],
  "font_size": 8,
  "threshold_potential_duplicate_color": 50,
  "threshold_potential_duplicate_embedding": 0.99,
  "use_file_paths_json": false,
  "file_paths_json_path": "file_paths.json",
  "text_embedding_search_presets": [
    "cute cat",
    "fine gentleman",
    "cool car",
    {"positive": "masterpiece, artful", "negative": "ugly, deformed"}
  ],
  "image_classifier_models": [
    {
      "model_name": "h5 model name (be sure to change this)",
      "model_location": "path/to/my/model.h5",
      "model_categories": ["photograph", "drawing"],
      "use_hub_keras_layers": false
    },
    {
      "model_name": "safetensors model name (be sure to change this)",
      "model_location": "path/to/my/model.safetensors",
      "model_categories": ["category1", "category2", "category3"],
      "model_kwargs": {
        "architecture_module_name": "my_model_architecture_module",
        "architecture_class_path": "MyModelArchitectureClass (optional)",
        "architecture_location": "path/to/my/model_architecture.py (optional)"
      }
    }
  ],
  "text_embedding_search_presets_exclusive": false,
  "enable_prevalidations": true,
  "show_negative_prompt": true,
  "sd_runner_client_port": 6000,
  "sd_runner_client_password": "<PASSWORD>",
  "refacdir_client_port": 6001,
  "refacdir_client_password": "<PASSWORD>",
  "gimp_exe_loc": "gimp-3.0",
  "image_edit_configuration": {
    "random_rotation_chance": 0.5,
    "random_flip_chance": 0.5,
    "random_draw_chance": 0.5,
    "random_crop_chance": 0.5,
    "random_color_rotation_chance": 0.2,
    "random_scale_chance": 0.3,
    "random_noise_chance": 0.7,
    "random_contrast_chance": 0.2,
    "random_brightness_chance": 0.1,
    "random_saturation_chance": 0.4,
    "random_hue_chance": 0.5,
    "random_blur_chance": 0.3,
    "random_sharpen_chance": 0.7,
    "random_invert_chance": 0.2,
    "random_sepia_chance": 0.1,
    "random_grayscale_chance": 0.5,
    "random_pixelate_chance": 0.3,
    "texture_draw_probability": 0.6
  }
}
